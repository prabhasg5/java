//Write a Java program to find the sum of all the elements of an array.
import static java.lang.System.out;
import java.util.*;
class sum
{
    int size,sum;
    int arr[]=new int[20];
    Scanner scanf=new Scanner(System.in);
    public void input()
    {
        out.println("enter the size of the array");
        size=scanf.nextInt();
        for(int i=0;i<size;i++)
        {
            arr[i]=scanf.nextInt();
        }
    }
    public void add()
    {
        for(int i:arr)
        {
        sum +=i;
        }
        out.println("sum = "+sum);
    }
    public static void main(String a[])
    {
        sum sum=new sum();
        sum.input();
        sum.add();
    }
}
